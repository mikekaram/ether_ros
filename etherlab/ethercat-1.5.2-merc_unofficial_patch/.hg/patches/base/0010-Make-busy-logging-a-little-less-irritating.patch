         if (!ec_fsm_slave_is_ready(&slave->fsm)) {
-            EC_SLAVE_DBG(slave, 1, "Busy - processing external request!\n");
+            EC_SLAVE_DBG(slave, 2, "Busy - processing external request!\n");
