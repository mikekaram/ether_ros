         list_for_each_entry(req, &slave->config->sdo_requests, list) {
             if (req->state == EC_INT_REQUEST_QUEUED || req->state == EC_INT_REQUEST_BUSY) {
-                EC_SLAVE_DBG(slave, 1, "Busy - processing internal SDO request!\n");
